# Gemini API (required)
GEMINI_API_KEY=your_gemini_api_key_here

# DELTA3 API Server (required for client)
DELTA3_API_URL=http://localhost:8000
DELTA3_API_KEY=your_delta3_api_key_here

# Server Configuration (for running API server)
SECRET_KEY=your_secret_key_for_jwt
S3_BUCKET=your-s3-bucket-name
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Firecracker paths (server only)
KERNEL_PATH=/opt/firecracker/vmlinux
BASE_ROOTFS_PATH=/opt/firecracker/rootfs.ext4
